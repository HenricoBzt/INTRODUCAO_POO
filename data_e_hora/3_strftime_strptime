from datetime import datetime

data_hora_atual = datetime.now()
data_hora_str   = "2024-09-23 19:57"
pt_br = "%d/%m/%Y %H:%M %a"
en    ="%Y-%m-%d %H:%M"

print(data_hora_atual.strftime(pt_br))  #Formtando data e hora


data_convertida = datetime.strptime(data_hora_str,en) #Convetrte uma string para datetime
print(data_convertida)